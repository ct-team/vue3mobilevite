{
  "name": "vue3admvite",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vite --mode development",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build:online": "vite build --mode production && gulp move --env production",
    "build:pre": "vite build --mode pre && gulp move --env pre",
    "build:test": "vite build --mode ctest && gulp move --env ctest",
    "build:dev": "vite build --mode dev && gulp move --env dev",
    "build:inner": "vite build --mode stable && gulp move --env stable"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nat-plus": "^0.2.2",
    "pinia": "2.0.36",
    "sass": "^1.64.1",
    "vant": "^4.6.3",
    "vconsole": "^3.15.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "glob": "^10.3.3",
    "gulp": "^4.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.8.8",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-external-globals": "^0.8.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "typescript": "~5.0.4",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-ejs": "^1.6.4",
    "vite-plugin-mpa-plus": "^0.5.4",
    "vue-tsc": "^1.6.5"
  }
}
